        {% include 'layouts/footer.html.twig' %}

        <script src="{{ path }}/js/jquery.js"></script>
        <script src="{{ path }}/js/popper.js"></script>
        <script src="{{ path }}/js/bootstrap.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/sweetalert2@8"></script>
